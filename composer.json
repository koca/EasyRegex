{
    "name": "koca/easyregex",
    "license": "MIT",
    "type": "library",
    "description": "PHP Regular expressions made easy",
    "keywords": ["regex"],
    "require": {
        "php": ">=5.3",
        "fabpot/php-cs-fixer": "^1.11"
    },
    "require-dev": {
        "phpunit/phpunit": "* >=4"
    },
    "suggest": {
        "fabpot/php-cs-fixer": "PHP CS Fixer (testing for PSR-2 compliance)"
    },
    "autoload": {
        "psr-4": {
            "Koca\\EasyRegex\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
